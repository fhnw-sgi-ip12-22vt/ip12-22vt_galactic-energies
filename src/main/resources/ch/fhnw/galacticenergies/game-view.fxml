<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Scene?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="rootAnchor" xmlns:fx="http://javafx.com/fxml" fx:controller="ch.fhnw.galacticenergies.controllers.GameViewController" style="-fx-background-image: url('https://images.unsplash.com/photo-1534796636912-3b95b3ab5986?ixlib=rb-4.0.3'); -fx-background-size: cover">
    <AnchorPane fx:id="spaceanchor" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0">
        <children>
            <ImageView fx:id="spaceshipimgview" rotate="90" fitWidth="100" preserveRatio="true">
                <image>
                    <Image fx:id="spaceshipimg" url="@images/Rakete.png" backgroundLoading="false"/>
                </image>
            </ImageView>
        </children>
    </AnchorPane>
</AnchorPane>
